/**
 * @context face
 * @imports metal.jax.System
 * @imports metal.jax.Face
 * @requires metal.jax.Face ${base}
 * @source base=${baseURL} module=${baseModule} includes=[.]test[.]
 * @source base=${baseURL} module=${baseModule} includes=Test$
 * @source base=${baseURL} module=metal-face includes=metal.face
 */

//@private
function initClass() {
	parseResourceModel(Face, this, System.document.body);
}

//@private
function initContext() {
	var clazz = extendClass(Face);
	var base = $("base");
	if (base) {
		System.document.body.appendChild(clazz.fromResource(base).node);
	} else {
		new clazz(System.document.body);
	}
}
