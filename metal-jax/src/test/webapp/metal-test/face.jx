/**
 * @context face
 * @imports ${baseClass} Base
 * @requires metal.jax.Face ${base}
 * @source base=${baseURL} module=${baseModule} includes=[.]test[.]
 * @source base=${baseURL} module=${baseModule} includes=Test$
 * @source base=${bootURL} module=metal-face includes=^metal.face
 */

//@private
function initClass() {
	Face.parseModel(this, document.body);
}

//@private
function initContext() {
	var Face0 = extendClass(Face);
	var base = $("base");
	if (base) {
		if ((Base||Face0).findResource(base)) {
			document.body.appendChild((Base||Face0).fromResource(base).node);
		} else {
			log("warn", "metal.jax.init.base.fail", base);
		}
	} else {
		new Face0(document.body);
	}
}
