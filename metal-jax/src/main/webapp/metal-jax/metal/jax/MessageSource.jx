/**
 * @resource MessageSource
 * 
 * @setting resource.module=messages
 * @setting resource.type=properties
 */

//@private
function initResource(name, scope) {
	var messages = this.findResource(name, true);
	for (var key in messages) {
		if (!(key in scope.$setting)) {
			scope.$setting[key] = messages[key];
		}
	}
}
