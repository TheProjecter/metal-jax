/**
 * @context test.metal
 * @imports metal.test.TestRunner
 * @source base=${basePath} folder=${mainFolder} excludes=^metal[.]
 * @content type=test extends=metal.test.TestCase
 */

//@private
function initObject() {
	this.set("testRunner", new TestRunner());
}

//@private
function initContext() {
	this.get("testRunner").run($context);
}
