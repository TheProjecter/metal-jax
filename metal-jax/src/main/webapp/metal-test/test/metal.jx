/**
 * @context test.metal
 * @imports metal.test.TestSuite
 * @source base=${basePath} folder=${mainFolder} excludes=^metal[.]
 * @content type=test extends=metal.test.TestCase
 */

//@private
function initObject() {
	this.set("testSuite", new TestSuite());
}

//@private
function initContext() {
	this.get("testSuite").run($context);
}
