/**
 * @service Echo
 * @imports model.HelloModel
 * @imports model.Hello
 * @setting service.base=http://metal-jax.appspot.com
 * @setting service.path=model/Hello.hello.xml
 * @setting service.module=metal-model
 */

//@public
function hello(model, callback) {
	var source = initSource.call(this, new HelloModel(model), "hello", Hello);
	call.call(this, source, callback);
}
