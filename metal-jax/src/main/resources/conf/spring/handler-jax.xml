<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="jax.crossDomainRequestHandler" class="metal.jax.web.handler.CrossDomainRequestHandler">
		<property name="version" value="metal-jax"/>
	</bean>
	
    <bean id="jax.serviceRequestHandler" class="metal.jax.web.handler.ServiceRequestHandler">
    	<property name="service" ref="jax.ModelService"/>
    	<property name="serviceInterface" value="metal.jax.service.ModelService"/>
		<property name="serviceMap">
			<map>
				<entry key="/model"><ref bean="jax.ModelService"/></entry>
				<entry key="/service" value="default"/>
			</map>
		</property>
    </bean>
	
</beans>
