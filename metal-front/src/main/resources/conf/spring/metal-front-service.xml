<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="metal.front.crossDomainRequestHandler" class="metal.front.crossdomain.CrossDomainRequestHandler">
		<property name="version" value="metal-jax"/>
	</bean>
	
	<bean id="metal.front.serviceRequestHandler" class="metal.front.service.ServiceRequestHandler">
		<property name="serviceMap">
			<map>
				<entry key="/service"><bean class="metal.front.service.ServiceSetting"/></entry>
				<entry key="/model">
					<bean class="metal.front.service.ServiceSetting">
						<property name="servicePath" value="/metal/jax/model"/>
						<property name="methodMap">
							<map>
								<entry key="GET" value="load"/>
							</map>
						</property>
					</bean>
				</entry>
			</map>
		</property>
    </bean>
	
</beans>
